{
  "name": "next-starter-template",
  "license": "UNLICENSED",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "test:output": "jest --json --outputFile=.jest-test-results.json",
    "test-storybook": "test-storybook",
    "test-storybook:watch": "test-storybook --watch",
    "test:coverage": "jest --coverage ",
    "test:cover": "istanbul cover --include-all-sources spec/specs.js && istanbul check-coverage",
    "prepare": "husky install",
    "pre-commit": " git add . && lint-staged",
    "commit": "git add . && cz",
    "push": "git push origin ",
    "pull": "git pull origin",
    "type-check": "tsc --noEmit ",
    "analyze": "ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.4",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "postcss-syntax": "^0.36.2",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@babel/core": "^7.18.13",
    "@cypress/code-coverage": "^3.10.0",
    "@next/eslint-plugin-next": "^12.2.5",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-toolbars": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/test-runner": "^0.7.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.7.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.4",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^8.0.1",
    "istanbul": "^0.4.5",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "storybook-addon-performance": "^0.16.1",
    "storybook-addon-pseudo-states": "^1.15.1",
    "typescript": "^4.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
